<h1>Edit Order</h1>
 
<%= form_with(model: @order) do |f| %>
 
  
 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :order_details %><br>
    <%= f.text_area :order_details %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
 <%= link_to 'Back', orders_path %>